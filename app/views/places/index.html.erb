<%# render category list %>
<% @places.each do |place| %>
  <% place.posts.each do |post| %>
    <%= cl_video_tag(
      post.video.key,
      autoplay: true,
      muted: true,
      loop: true,
      controls: true,
      fallback_content: "Your browser does not support HTML5 video tags." )
     %>
  <% end %>
<% end %>
